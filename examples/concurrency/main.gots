package main

import fmt from "fmt"

function producer(out: chan<number>): void {
	send out => 3
	send out => 7
}

function consumer(in: chan<number>): void {
	let a = await in
	let b = await in
	fmt.Println("received", a, b)
}

function main(): void {
	let ch: chan<number> = makeIntChannel(4)
	spawn producer(ch)
	spawn consumer(ch)
}
